<p>いいねした投稿一覧</p>
<% @favorite_posts.each do |post| %>
<div>
  <%= link_to post_path(post.id) do %>
    <%= attachment_image_tag post, :image, format: 'jpeg', size: "100x100"  %>
  <p>タイトル：<%= post.title %></p>
  <% end %>
  ♥<%= post.favorites.count %>いいね
</div>
<% end %>
<%= link_to "投稿一覧", posts_path(@post) %>
<%= paginate @posts %>